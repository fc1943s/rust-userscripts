{
  "name": "rust-userscripts-host",
  "version": "0.1.0",
  "description": "rust-userscripts-host",
  "author": "Filipe Carvalho <fc1943s@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fc1943s/rust-userscripts.git"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "rust",
    "webpack"
  ],
  "bin": {
    "create-wasm-app": ".bin/create-wasm-app.js"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js && node update-version.js ../userscripts/filmow.user.js",
    "build-filmow-dev": "webpack --config webpack.config.js && rollup --config && node update-version.js ../userscripts/filmow-dev.user.js && node ../userscripts/build-filmow-dev.js",
    "build2": "rollup --config",
    "start": "webpack-dev-server"
  },
  "dependencies": {
    "rust-userscripts": "file:../pkg"
  },
  "devDependencies": {
    "hello-wasm-pack": "^0.1",
    "webpack": "^4.44",
    "webpack-cli": "^3.3",
    "webpack-dev-server": "^3.11",
    "copy-webpack-plugin": "^6.0",
    "@wasm-tool/rollup-plugin-rust": "^1.0",
    "replace-in-file": "^6.1"
  }
}
